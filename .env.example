# H2OMoney Environment Variables
# Copy this file to .env and fill in your values

# ========== Telegram ==========
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# ========== Sui Network ==========
# Options: mainnet, testnet, devnet, localnet
SUI_NETWORK=testnet

# Your wallet private key (KEEP THIS SECRET!)
# Format: suiprivkey... or 0x...
SUI_PRIVATE_KEY=your_private_key_here

# ========== Cetus ==========
# Cetus Aggregator SDK endpoint (router_v3)
CETUS_AGGREGATOR_URL=https://api-sui.cetus.zone/router_v3

# ========== stablelayer ==========
# StableLayer SDK BrandUSD coin type
STABLELAYER_BRAND_USD_TYPE=0x...::brand_usd::BRAND_USD
# BrandUSD decimals (default: 6)
STABLELAYER_BRAND_USD_DECIMALS=6

# ========== Optional ==========
# Logging level: debug, info, warn, error
LOG_LEVEL=info

# Web dashboard port
WEBAPP_PORT=3000

# Bot server port
BOT_PORT=3001
